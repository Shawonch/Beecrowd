#include<iostream>
using namespace std;
int main()
{
    int i,n,m,a=0,b=0,c=0,d=0;
    cin>>n;
    for(i=1;i<=n;i++)
    {
        cin>>m;
       if(m%2==0)
       {
           a++;
       }
       if(m%2!=0)
       {
           b++;
       }
       if(m>0)
       {
           c++;
       }
       if(m<0)
       {
           d++;
       }
    }
    cout<<"Even: "<<a<<endl<<"Odd: "<<b<<endl<<"Positive: "<<c<<endl<<"Negative: "<<d;
 
}
