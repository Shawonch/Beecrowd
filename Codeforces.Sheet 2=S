#include<bits/stdc++.h>
using namespace std;
int main()
{
  int n,i,a,b,Max,Min,j;
  cin>>n;
  for(i=1;i<=n;i++)
  {
      int sum=0;
      cin>>a>>b;
      Max=max(a,b);
      Min=min(a,b);
      for(j=Min+1;j<Max;j++)
      {
          if(j%2!=0)
 
              sum=sum+j;
 
      }
      cout<<sum<<endl;
  }
    return 0;
}
