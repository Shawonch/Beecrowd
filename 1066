#include<stdio.h>
int main()
{
    int i,n=5,m,a=0,b=0,c=0,d=0;
    for(i=0; i<n; i++)
    {
        scanf("%d",&m);
        if(m%2==0)
        {
            a++;
        }
        if(m%2 !=0)
        {
            b++;
        }
         if(m>0)
        {
            c++;
        }
         if(m<0)
        {
            d++;
        }

    }
    printf("%d valor(es) par(es)\n",a);
    printf("%d valor(es) impar(es)\n",b);
    printf("%d valor(es) positivo(s)\n",c);
    printf("%d valor(es) negativo(s)\n",d);

    return 0;
}
